generator client {
  provider = "prisma-client-js"
}

// prisma/schema.prisma
datasource db {
  provider  = "postgresql"
  url  	    = env("DATABASE_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}
model Devise {
  id        String   @id @default(uuid())
  nom       String
  code      String   @unique
  unite     Float
  achat     Float
  vent      Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
